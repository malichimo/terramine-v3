{
  "expo": {
    "name": "TerraMine",
    "slug": "TerraMine",
    "scheme": "terramine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.terramine.app",
      "config": {
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.terramine.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "${AIzaSyCWOew8UMeRv3Xw4yVTrl55pAufJD3I1lQ}"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with the property owner.",
          "cameraPermission": "The app accesses your camera to let you take check-in photos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7137937a-c2a0-4f44-82fb-66ae790710a1"
      }
    }
  }
}
